{
    "env": {
        "node": true
    },
    "rules": {
        "func-names": [0],
        "no-console": [0],
        "no-process-exit": [0],
        "strict": [0],
        "import/newline-after-import": [0],
        "import/no-extraneous-dependencies": [2, { "devDependencies": true, "optionalDependencies": false }]
    }
}
